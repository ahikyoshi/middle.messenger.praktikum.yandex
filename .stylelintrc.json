{
    "extends": [
        "stylelint-config-standard",
        "stylelint-config-rational-order"
    ],
    "plugins": [
        "stylelint-order",
        "stylelint-scss"
    ],
    "rules": {
        "import-notation": null,
        "selector-class-pattern": null,
        "font-family-no-missing-generic-family-keyword": null
    }
}