{
  "name": "chats",
  "version": "0.2.1",
  "description": "Yandex Pract",
  "scripts": {
    "build": "parcel build ./src/index.html",
    "server": "node ./server/index.js",
    "dev": "parcel ./src/index.html --no-cache --port 3000",
    "eslint": "npx eslint ./src"
  },
  "engines": {
    "node": ">=12",
    "browsers": "> 0.25%"
  },
  "targets" : {
    "regular" : {
      "context" : "browser",
      "outputFormat" : "global",
      "isLibrary" : false,
      "includeNodeModules" : {
        "jquery": false
      }
    }
  },
  "type": "module",
  "author": "ahiky (ahikyoshi@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@parcel/transformer-sass": "2.8.2",
    "@parcel/transformer-typescript-tsc": "2.8.2",
    "express": "4.18.2",
    "nanoid": "4.0.1",
    "parcel": "2.8.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "parcel-plugin-nuke-dist": "1.0.1",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1"
  }
}
