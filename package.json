{
  "name": "chats",
  "version": "0.2.1",
  "description": "Yandex Pract",
  "scripts": {
    "build": "webpack --config ./configs/webpack.config.js",
    "dev": "webpack-dev-server --config ./configs/webpack.config.js",
    "test": "node ./node_modules/mocha/bin/mocha --loader=ts-node/esm",
    "prepare": "husky install",
    "ts:check": "tsc --noEmit",
    "lint": "eslint  src/ --ext .ts",
    "lint:styles": "stylelint  src/**/*.scss"
  },
  "engines": {
    "node": ">=12",
    "browsers": "> 0.25%"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!nanoid)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy",
      "^core/tests/(.*)$": "<rootDir>/src/core$1",
      "^utils(.*)$": "<rootDir>/src/utils$1",
      "^types(.*)$": "<rootDir>/src/types$1",
      "^pages(.*)$": "<rootDir>/src/pages$1",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^img(.*)$": "<rootDir>/src/img$1",
      "^routes(.*)$": "<rootDir>/src/routes$1",
      "^vendor(.*)$": "<rootDir>/src/vendor$1",
      "^api(.*)$": "<rootDir>/src/api$1",
      "^services(.*)$": "<rootDir>/src/services$1"
    }
  },
  "targets": {
    "regular": {
      "context": "browser",
      "outputFormat": "global",
      "isLibrary": false,
      "includeNodeModules": {
        "jquery": false
      }
    }
  },
  "author": "ahiky (ahikyoshi@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@types/jest": "^29.5.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "babel": "^6.23.0",
    "babel-jest": "^29.5.0",
    "eslint": "8.34.0",
    "express": "4.18.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nanoid": "4.0.1",
    "parcel": "2.8.2",
    "parcel-plugin-nuke-dist": "1.0.1",
    "proxyquire": "^2.1.3",
    "sinon": "^15.0.4",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-config-standard-scss": "^9.0.0",
    "ts-jest": "^29.1.0",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.14",
    "babel-cli": "^6.26.0",
    "chai": "^4.3.7",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "html-webpack-plugin": "^5.5.1",
    "husky": "^8.0.0",
    "mini-css-extract-plugin": "^2.7.5",
    "mocha": "^10.2.0",
    "prettier": "^2.8.8",
    "sass-loader": "^13.2.2",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  }
}
