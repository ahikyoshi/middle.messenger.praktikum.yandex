{
  "name": "chats",
  "version": "0.2.1",
  "description": "Yandex Pract",
  "scripts": {
    "build": "parcel build ./src/index.html ",
    "devServer": "parcel ./src/index.html  -p 3000",
    "server": "node server.js",
    "eslint": "npx eslint ./src"
  },
  "engines": {
    "node": ">=12"
  },
  "targets": {
    "regular": {
      "source": "src/index.ts",
      "context": "browser",
      "outputFormat": "global",
      "isLibrary": false
    }
  },
  "author": "ahiky (ahikyoshi@gmail.com)",
  "license": "ISC",
  "dependencies": {
    "@parcel/transformer-sass": "2.8.2",
    "@parcel/transformer-typescript-tsc": "2.8.2",
    "express": "4.18.2",
    "nanoid": "4.0.1",
    "nodemon": "^2.0.21",
    "parcel": "2.8.2"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "parcel-plugin-nuke-dist": "1.0.1",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1"
  }
}
