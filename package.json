{
  "name": "chats",
  "version": "0.2.1",
  "description": "Yandex Pract",
  "scripts": {
    "build": "webpack --config ./configs/webpack.config.js",
    "dev": "webpack serve --config ./configs/webpack.config.js",
    "server": "node ./server/index.js",
    "eslint": "npx eslint ./src"
  },
  "engines": {
    "node": ">=12",
    "browsers": "> 0.25%"
  },
  "targets": {
    "regular": {
      "context": "browser",
      "outputFormat": "global",
      "isLibrary": false,
      "includeNodeModules": {
        "jquery": false
      }
    }
  },
  "type": "commonjs",
  "author": "ahiky (ahikyoshi@gmail.com)",
  "license": "ISC",
  "devDependencies": {
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "8.34.0",
    "html-webpack-plugin": "5.5.1",
    "mini-css-extract-plugin": "2.7.5",
    "nanoid": "4.0.1",
    "sass-loader": "13.2.2",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "ts-loader": "9.4.2",
    "webpack": "5.80.0",
    "webpack-cli": "5.0.2",
    "webpack-dev-server": "4.13.3"
  }
}
